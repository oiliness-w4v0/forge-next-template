{
  "name": "yxswy",
  "version": "1.0.0",
  "main": "./.vite/build/main.js",
  "type": "module",
  "devDependencies": {
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@types/bun": "^1.3.8",
    "electron": "^40.1.0"
  },
  "scripts": {
    "build:main": "bun run ./build.ts",
    "build:renderer": "vite build",
    "build": "bun run build:renderer && bun run build:main",
    "dev": "bun run build:main && (vite --host & electron-forge start)",
    "make": "bun run build && electron-forge make",
    "start": "electron-forge start"
  },
  "dependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/shared-types": "^7.11.1",
    "vite": "^7.3.1"
  }
}